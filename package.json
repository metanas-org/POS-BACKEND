{
	"name": "multi-tenants",
	"version": "1.0.0",
	"description": "",
	"main": "app.ts",
	"scripts": {
		"start": "npm run build:ts && fastify start -a '0.0.0.0' -l info dist/app.js",
		"build:ts": "rm -rf dist && tsc",
		"watch:ts": "tsc -w",
		"dev": "concurrently \"npm run watch:ts\" \"fastify start -w  -a '0.0.0.0' --ignore-watch=.ts$  -l info -P dist/app.js\"",
		"prepare": "husky install",
		"migrate": "npx knex migrate:latest --knexfile ./src/database/knexfile.ts ",
		"rollback": "npx  knex migrate:rollback --all --knexfile ./src/database/knexfile.ts",
		"seed":"npx knex seed:run --knexfile ./src/database/knexfile.",
		"migrate:tenants": "ts-node ./src/scripts/migrate.ts migrate",
		"seed:create": "ts-node ./src/scripts/seed.ts make",
		"rollback:tenants": "ts-node ./src/scripts/migrate.ts rollback",
		"seed:tenants": "ts-node ./src/scripts/seed.ts seed"
	},
	"keywords": [
		"fastify",
		"typescript",
		"knex"
	],
	"author": "ray",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^5.7.1",
		"@fastify/cors": "^8.3.0",
		"@fastify/helmet": "^11.0.0",
		"@fastify/multipart": "^7.7.3",
		"@fastify/rate-limit": "^8.0.3",
		"@fastify/sensible": "^5.2.0",
		"@fastify/swagger": "^8.8.0",
		"@fastify/swagger-ui": "^1.9.3",
		"fastify": "^4.21.0",
		"fastify-plugin": "^4.5.1",
		"fluent-json-schema": "^4.1.1",
		"knex": "^2.5.1",
		"objection": "^3.1.1",
		"pg": "^8.11.2"
	},
	"devDependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/node": "^20.5.0",
		"@types/tap": "^15.0.8",
		"concurrently": "^8.2.0",
		"dotenv": "^16.3.1",
		"fastify-cli": "^5.8.0",
		"fastify-tsconfig": "^1.0.1",
		"husky": "^8.0.3",
		"pretty-quick": "^3.1.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	}
}
